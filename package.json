    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "lerna": "^6.0.1",
    "lint-staged": "^13.0.0",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0"
  }
