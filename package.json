    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.15.1",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5",
    "jest": "^23.5.0",
    "lerna": "^3.1.4",
    "lint-staged": "^7.2.2",
    "path": "^0.12.7",
    "prettier": "^1.10.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "terser-webpack-plugin": "^2.1.2",
    "ts-jest": "^23.10.5",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 5.6.0"
  }
}
