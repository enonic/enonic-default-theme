{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "coverage",
    "test",
    "bin",
    ".vscode"
  ],
  "types": [
    "types"
  ]
}
